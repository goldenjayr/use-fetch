    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@goldenjayr/use-fetch",
  "repository": {
    "type": "git",
    "url": "https://github.com/goldenjayr/use-fetch"
  },
  "author": "Jay-R Joseph Gabunada",
  "module": "dist/use-fetch.esm.js",
  "size-limit": [
    {
      "path": "dist/use-fetch.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-fetch.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/lru-cache": "^5.1.0",
    "@types/md5": "^2.3.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lru-cache": "^6.0.0",
    "md5": "^2.3.0"
  }
}
